--TEST--
Test color optimisation
--CSS--
a { 
    foo:rgb(0,0,0);
    bar:black;
    baz:#ffaabb;
    qux:#f00;
    quux:blanchedalmond;
    quuux:rgb(0,0,1);
    acci:hsl(0, 100%, 50%);
}
--EXPECT--
array (
  'a' => 
  array (
    'foo' => '#000',
    'bar' => '#000',
    'baz' => '#fab',
    'qux' => 'red',
    'quux' => '#ffebcd',
    'quuux' => '#000001',
    'acci' => 'red',
  ),
)
