--TEST--
Test color optimisation
--CSS--
a { 
    foo:rgb(0,0,0);
    bar:black;
    baz:#ffaabb;
    qux:#f00;
    quux:blanchedalmond;
    rgb:rgb(0,0,1);
    rgb2:rgb(255,255,100%);
    hsl:hsl(25, 100%, 50%);
    hsl2:hsl(210, 100%, 50%);
    hsl3:hsl(270, 25%, 25%);
}
--EXPECT--
array (
  'a' => 
  array (
    'foo' => '#000',
    'bar' => '#000',
    'baz' => '#fab',
    'qux' => 'red',
    'quux' => '#ffebcd',
    'rgb' => '#000001',
    'rgb2' => '#fff',
    'hsl' => '#ff6a00',
    'hsl2' => '#0080ff',
    'hsl3' => '#403050',
  ),
)
