--TEST--
@font-face rules
--CSS--
@font-face {
    font-family: GillSans;
    src:url("/generic/fonts/GillSansLightC.otf") format('opentype', "svg");
    font-style: normal;
    font-weight: normal;
    unicode-range: U+00-FF, U+980-9FF;
}

@font-face {
    font-family: GillSans;
    src:url("/generic/fonts/GillSansC.otf") format('opentype');
    font-style: normal;
    font-weight: bold;
}
